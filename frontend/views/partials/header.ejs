<header class="header">
  <div class="logo-container">
    <img src="/images/LOGO.png" alt="Campus Wellness Logo" class="logo-img">
    <div class="logo-text">
      <span class="logo-text-main">CAMPUS WELLNESS</span>
      <span class="logo-text-sub">IIT DHARWAD</span>
    </div>
  </div>
  <nav class="nav-buttons">
  <%- typeof navHtml !== 'undefined' ? navHtml : '' %>
  <% 
    var __uid = (typeof userid !== 'undefined' && userid) 
      ? userid 
      : (typeof user !== 'undefined' && user && user.id ? user.id : null);
  %>
  <% if (__uid) { %>
    <a href="/users/<%= __uid %>" class="btn">DASHBOARD</a>
  <% } %>
  <a href="/logout" class="btn">LOG OUT</a>
  </nav>
</header>
